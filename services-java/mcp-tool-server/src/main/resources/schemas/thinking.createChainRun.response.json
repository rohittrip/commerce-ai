{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["runId", "status", "stages"],
  "properties": {
    "runId": {
      "type": "string",
      "description": "Chain run ID"
    },
    "status": {
      "type": "string",
      "description": "Current status"
    },
    "stages": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "description": "List of thinking stages"
    }
  }
}
