{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "ok": {
      "type": "boolean"
    },
    "traceId": {
      "type": "string"
    },
    "data": {
      "type": "object",
      "properties": {
        "products": {
          "type": "array",
          "items": {
            "$ref": "./common.types.json#/definitions/ProductSummary"
          }
        },
        "total": {
          "type": "integer",
          "minimum": 0
        },
        "pagination": {
          "$ref": "./common.types.json#/definitions/Pagination"
        }
      },
      "required": ["products", "total"]
    },
    "error": {
      "$ref": "./common.types.json#/definitions/ToolError"
    }
  },
  "required": ["ok", "traceId"]
}
